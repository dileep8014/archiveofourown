{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-card": "^1.0.0-beta.2",
    "@ant-design/pro-field": "^0.0.12",
    "@ant-design/pro-layout": "^6.2.5",
    "@ant-design/pro-list": "^0.0.6",
    "@ant-design/pro-utils": "^0.0.2",
    "@types/mockjs": "^1.0.2",
    "@types/nprogress": "^0.2.0",
    "@types/react-virtualized": "^9.21.10",
    "@types/redux-logger": "^3.0.8",
    "@umijs/hooks": "^1.9.3",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.10",
    "ahooks": "^2.5.0",
    "antd": "^4.4.3",
    "antd-img-crop": "^3.9.0",
    "lint-staged": "^10.0.7",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-virtualized": "^9.21.2",
    "redux-logger": "^3.0.6",
    "slate": "^0.58.4",
    "slate-history": "^0.58.4",
    "slate-react": "^0.58.4",
    "umi": "^3.2.10",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@umijs/preset-ui": "^2.2.2"
  }
}
